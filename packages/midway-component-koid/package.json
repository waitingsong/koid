  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/waitingsong/koid/issues"
  },
  "homepage": "https://github.com/waitingsong/koid#readme",
  "repository": {
    "type": "git",
    "url": "git//github.com/waitingsong/koid.git"
  },
  "license": "MIT",
  "dependencies": {
    "@waiting/shared-core": "^15.1.0",
    "@waiting/shared-types": "^15.1.0",
    "koid": "^5.0.0"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.2.90",
    "@midwayjs/core": "^2.13.0",
    "@midwayjs/decorator": "^2.13.0",
    "@midwayjs/web": "^2.13.0",
    "@types/node": "16",
    "cross-env": "7",
    "egg": "^2.31.0"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "midway-bin build -c",
    "clean": "npm run clean:lock & npm run clean:cache & rm -rf dist/* -rf",
    "clean:cache": "rm -rf .eslintcache .vscode/.tsbuildinfo .vscode/.tsbuildinfo.*",
    "clean:lock": "rm package-lock.json -f",
    "cov": "cross-env TS_NODE_PROJECT=test/tsconfig.json c8 mocha --parallel=false",
    "lint": "eslint --fix --cache {src,test}/**/*.ts",
    "lint:nofix": "eslint --cache {src,test}/**/*.ts",
    "purge": "npm run clean && rm node_modules -rf",
    "rp": "rollup -c rollup.config.js",
    "test": "cross-env TS_NODE_PROJECT=test/tsconfig.json mocha",
    "test:local": "cross-env TS_NODE_PROJECT=test/tsconfig.json ../../node_modules/.bin/mocha --parallel=false",
    "tsc": "tsc -p tsconfig.json"
  }
}
